<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="Currrency" table="CURRENCY">
<meta attribute="class-description">
This class contains the currency detail.
</meta>
<id name="id" type="int" column="id">
<generator class="native"/>
</id>
<property name="countryId" column="country_id" type="int"/>
<property name="name" column="currency_name" type="string"/>
<property name="sign" column="shortcut" type="string"/>
</class>
</hibernate-mapping>