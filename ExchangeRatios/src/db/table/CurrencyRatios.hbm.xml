<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="CurrencyRatios" table="CURRENCY_RATIOS">
<meta attribute="class-description">
This class contains the currency-ratio detail.
</meta>
<id name="id" type="int" column="id">
<generator class="native"/>
</id>
<property name="currencyId" column="currency_id" type="int"/>
<property name="date" column="effective_date" type="string"/>
<property name="askPrice" column="ask_Price" type="numeric"/>
<property name="bidPrice" column="bid_price" type="numeric"/>
<property name="avgPrice" column="avg_price" type="numeric"/>
</class>
</hibernate-mapping>